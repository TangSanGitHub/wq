<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="wq.dao.WqDao">
    <insert id="doPostMessage">
        INSERT INTO wq(name,info,status)
        VALUES (#{message.name},#{message.info},'A')
    </insert>
</mapper>
